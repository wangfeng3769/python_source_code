<script type="text/javascript">
Until.init();
Until.getCars();
function getCarInfo(carId)
{

	Until.postJson("client/http/member.php",{item:'twGetCarInfo',car_id:carId},function(data){
		if (data.errno==4060)
		{
			window.location.href='login.html';
		}
	});
}

