{include file="pageheader.htm"}
<div class="main-div">
<form method="post" action="car_violate.php" name="theForm" enctype="multipart/form-data" onsubmit="return validate()">
<table cellspacing="1" cellpadding="3" width="100%">
  {if $data.uname}
  <tr>
    <td class="label">用户姓名:</td>
    <td><input type="text" name="uname" maxlength="60" value="{$data.uname}" {if $data.uname}readonly{/if}/></td>
  </tr>
  {/if}
  <tr>
    <td class="label">违章时间:</td>
    <td><input type="text" name="violate_time" maxlength="60" value="{$data.violate_time}"/>&nbsp;&nbsp;&nbsp;<a style="color:#aaa">YY-MM-DD HH:MM:SS</a></td>
  </tr>
  <tr>
    <td class="label">违章地点:</td>
    <td><input type="text" name="violate_location" maxlength="60" value="{$data.violate_location}" /></td>
  </tr>
  <tr>
    <td class="label">违章类型:</td>
    <td><input type="text" name="violate_type" maxlength="60" value="{$data.violate_type}"/></td>
  </tr>
  <tr>
    <td class="label">违章罚金:</td>
    <td><input type="text" name="violate_cost" maxlength="60" value="{$data.violate_cost}"/></td>
  </tr>
  <tr>
    <td class="label">车 牌 号:</td>
   <td><input type="text" name="car_number" maxlength="60" value="{$data.car_number}"/></td>
  </tr>
  <tr>
    <td class="label">扣分:</td>
   <td><input type="text" name="violate_point" maxlength="60" value="{$data.violate_point}"/></td>
    
  </tr>
   <tr>
    <td class="label">代办费用:</td>
   <td><input type="text" name="agency_cost" maxlength="60" value="{$data.agency_cost}"/></td>
  </tr>
  <tr>
    <td class="label">处理状态:</td>
   <td><select name="violate_stat">
    <option value="0" {if $data.process_stat==0}selected{/if}>未处理</option>
    <option value="1" {if $data.process_stat==1}selected{/if}>代办中</option>
    <option value="2" {if $data.process_stat==2}selected{/if}>已自行处理</option>
    <option value="3" {if $data.process_stat==3}selected{/if}>已代办处理</option>
   </select></td>
  </tr>

  {if $data.order_no}
   <tr>
    <td class="label">订单号:</td>
   <td><input type="text"  maxlength="60" value="{$data.order_no}"  {if $data.uname}readonly{/if}/></td>
  </tr>
     <tr>
    <td class="label">取车时间:</td>
   <td><input type="text"  maxlength="60" value="{$data.order_start_time}"  {if $data.uname}readonly{/if}/></td>
  </tr>
     <tr>
    <td class="label">还车时间:</td>
   <td><input type="text"  maxlength="60" value="{$data.order_end_time}"  {if $data.uname}readonly{/if}/></td>
  </tr>
     <tr>
    <td class="label">手机号码:</td>
   <td><input type="text"  maxlength="60" value="{$data.phone}"  {if $data.uname}readonly{/if}/></td>
  </tr>
  {/if}
  <tr>
    <td colspan="2" align="center"><br />
      <input type="submit" class="button" value="保存" />
      <input type="reset" class="button" value="返回" onclick="javascript:history.back(-1);"/>
      <input type="hidden" name="act" value="{$form_action}" />
      <input type="hidden" name="next_act" value="{$next_act}" />
      <input type="hidden" name="violate_id" value="{$data.violate_id}" />
    </td>
  </tr>
</table>
</form>
</div>
{insert_scripts files="../js/utils.js,validator.js"}
{literal}
<script language="JavaScript">
<!--
